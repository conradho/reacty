  "bugs": {
    "url": "https://github.com/conradho/reacty/issues"
  },
  "devDependencies": {
    "babel-core": "^6.1.21",
    "babel-preset-es2015": "~6.1.18",
    "babel-preset-react": "~6.1.18",
    "babelify": "~7.2.0",
    "browserify": "^12.0.1",
    "chai": "~3.4.1",
    "jsdom": "^7.0.2",
    "mocha": "~2.3.3",
    "react": "~0.14.2",
    "react-bootstrap": "~0.27.3",
    "react-dom": "~0.14.2",
    "requirejs": "~2.1.20"
  }
