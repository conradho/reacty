{
  "name": "reacty",
  "version": "0.0.0",
  "description": "a single page react webapp",
  "main": "bundle.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "build": "browserify -t [ babelify --presets [ react es2015 ] ] src/main.jsx -o bundle.js"
  },
  "babel": {
    "presets": ["react", "es2015"]
